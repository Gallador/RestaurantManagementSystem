<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="225.0" prefWidth="225.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ManagerLoginController">
   <children>
      <TextField fx:id="txtUsername" layoutX="38.0" layoutY="79.0" promptText="Username" />
      <PasswordField fx:id="txtPassword" layoutX="38.0" layoutY="117.0" promptText="Password" />
      <Button layoutX="64.0" layoutY="156.0" mnemonicParsing="false" onAction="#login" text="Manager Login" />
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="38.0" layoutY="44.0" prefHeight="17.0" prefWidth="149.0" text="Please login as manager" />
   </children>
</AnchorPane>
